{
  "version": 2,
  "functions": {
    "api/index.py": {
      "maxDuration": 60,
      "runtime": "@vercel/python@3.11.0"
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ]
}
